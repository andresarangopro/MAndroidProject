<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:clickable="true"
              android:clipToPadding="true"
              android:fitsSystemWindows="true"
              android:orientation="vertical"
              android:textColor="@color/white">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/main_menu_top">

        <TextView
            android:id="@+id/title_warehouse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:padding="5dp"
            android:text=""
            android:textColor="@color/black"
            android:textSize="@dimen/font_big"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right|bottom"
            android:padding="5dp"
            android:text="思创理德"
            android:textColor="@color/light_blue"
            android:textSize="12dp"/>

    </FrameLayout>


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/main_inbound"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_inbound_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="收货"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

                <TextView
                    android:id="@+id/main_delivery"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_out_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="发货"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

                <TextView
                    android:id="@+id/main_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_take_stock_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="盘点"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/main_bind"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_stock_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="绑定"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

                <TextView
                    android:id="@+id/main_detection"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_check_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="检测"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

                <TextView
                    android:id="@+id/main_inventory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_inventory_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="库存查询"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/main_find"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_find_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="找货"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

                <TextView
                    android:id="@+id/main_setting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_settings_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="设置"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

                <TextView
                    android:id="@+id/out_login"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/selector_main_logout_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text="登录"
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:visibility="gone">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@mipmap/main_default_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text=""
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@mipmap/main_default_btn"
                    android:gravity="bottom|right"
                    android:padding="5dp"
                    android:text=""
                    android:textColor="@drawable/selector_color_text"
                    android:textSize="@dimen/font_big"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>


</LinearLayout>
