<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <View
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:background="@color/bg_color_base"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:padding="6dp"
        android:background="@color/bg_color_base">
        <ImageView
            android:layout_marginLeft="10dp"
            android:id="@+id/iv_read_back"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_centerVertical="true"
            android:src="@mipmap/arrow_left"/>
        <TextView
            android:id="@+id/tv_read_reading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="正在读取标签..."
            android:textColor="@color/white"
            android:layout_centerInParent="true"
            />
        <ImageView
            android:layout_marginRight="10dp"
            android:id="@+id/iv_read_upload"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/cloud_upload"/>
    </RelativeLayout>


   <FrameLayout
        android:id="@+id/stock_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </FrameLayout>
   <!-- <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.lide.app.ui.fragment.StockReadFragment"></fragment>-->

</LinearLayout>